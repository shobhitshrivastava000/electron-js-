{
  "name": "electron",
  "productName": "Electron Test",
  "version": "1.0.0",
  "description": "For Learning Purpose Only",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.electron.test",
    "mac": {
      "category": "public.app-category.utilities",
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    },
    "nsis": {
      "include": "build/installer.nsh"
    }
  },
  "author": "Krushita gajera",
  "license": "MIT",
  "dependencies": {
    "crypto": "^1.0.1",
    "electron-audio-loopback": "^1.0.6"
  },
  "devDependencies": {
    "electron": "^39.2.5",
    "electron-builder": "^26.0.12"
  }
}